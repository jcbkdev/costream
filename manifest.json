{
    "manifest_version": 3,
    "name": "Co-Stream",
    "version": "0.1.0",
    "description": "Watch co-streaming streamers simultaneously",
    "content_scripts": [
        {
            "matches": ["https://*.twitch.tv/*"],
            "js": ["build/bundle.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "https://*.twitch.tv/*"
    ],
    "icons": {
        "16": "build/costream-icon16.png",
        "32": "build/costream-icon32.png",
        "48": "build/costream-icon48.png",
        "128": "build/costream-icon128.png"
    }
}